{
  "manifest_version": 3,
  "name": "EasyIntros Alpha Extension",
  "version": "0.0.5",
  "description": "Alpha Cold Outreach Automation Chrome Extension",
  "permissions": [
    "activeTab",
    "sidePanel",
    "tabs",
    "scripting",
    "storage",
    "unlimitedStorage",
    "cookies"
  ],
  "host_permissions": [
    "*://*/*"
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  },
  "icons": {
    "16": "Arowwai.png",
    "48": "Arowwai.png",
    "128": "Arowwai.png"
  },
  "side_panel": {
    "default_path": "index.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ]
}

